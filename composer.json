{
    "name": "blockbrawn/npcdialogue",
    "description": "A PocketMine-MP library for making NPC dialogues",
    "type": "library",
    "license": "GPL-3.0-only",
    "minimum-stability": "dev",
    "require": {
        "sof3/await-generator": "^3.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "nethergamesmc/pocketmine-mp": "dev-stable"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:BlockBrawn/PocketMine-MP.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "cosmicpe\\npcdialogue\\": "src/cosmicpe/npcdialogue"
        }
    },
    "authors": [
        {
            "name": "Muqsit",
            "email": "notmuqsit@gmail.com"
        }
    ],
    "extra": {
        "virion": {
            "spec": "3.0",
            "namespace-root": "cosmicpe\\npcdialogue"
        }
    }
}
